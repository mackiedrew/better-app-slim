    "eslint-plugin-you-dont-need-lodash-underscore": "6.4.0",
    "flow-bin": "0.78.0",
    "prettier": "1.14.0"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
